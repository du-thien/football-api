{
  "kind": "collectionType",
  "collectionName": "matches",
  "info": {
    "name": "match",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "mid": {
      "type": "integer",
      "unique": true,
      "private": true
    },
    "sid": {
      "type": "integer"
    },
    "utcDate": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "SCHEDULED",
        "LIVE",
        "IN_PLAY",
        "PAUSED",
        "FINISHED",
        "POSTPONED",
        "SUSPENDED",
        "CANCELED"
      ]
    },
    "matchday": {
      "type": "integer",
      "default": 0
    },
    "odds": {
      "type": "json"
    },
    "score": {
      "type": "json"
    },
    "referees": {
      "type": "json"
    },
    "homeTeam": {
      "model": "team"
    },
    "awayTeam": {
      "model": "team"
    }
  }
}
